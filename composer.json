{
	"name":"bugbuster/contao_grid_16_columns",
	"description":"Adds a responsive 16-column pixel/percental grid with 1120px/1140px in the layout css-framework.",
	"keywords":["contao","grid","responsive"],
	"type":"contao-module",
	"homepage":"http://contao.ninja",
	"license":"LGPL-3.0+",
	"authors":[
		{
			"name":"Glen Langer",
			"homepage":"http://contao.ninja",
			"role":"Developer"
		}
	],
	"support":{
		"issues":"https://github.com/BugBuster1701/contao_grid_16_columns/issues",
		"irc":"irc://irc.freenode.org/contao.dev",
		"source":"https://github.com/BugBuster1701/contao_grid_16_columns",
		"forum":"https://community.contao.org/de/forumdisplay.php?119-Sonstige-Erweiterungen"
	},
	"require":{
		"php":">=5.3",
		"contao/core":">=3.3,<4",
		"contao-community-alliance/composer-plugin": "*"
	},
	"autoload":{
		"classmap":[""]
	},
	"replace":{
		"contao-legacy/grid_16_columns":"*"
	},
	"extra":{
		"contao":{
			"sources":{
				"system/modules/grid_16_columns":"system/modules/grid_16_columns"
			},
			"files":{
				"assets/contao/css":"assets/contao/css",
				"system/bin":"system/bin"
			},
			"transifex": {
				"project": "contao-grid_16_columns",
				"prefix": "master-",
				"languages_cto": "system/modules/grid_16_columns/languages",
				"languages_tx": ".tx"
			},
			"runonce":[
				"runonce/runonce.php"
			]
		}
	}
}
